class Calculator:
    def __init__(self):
        pass

    def add(self, a, b):
        return a + b

    def subtract(self, a, b):
        return a - b

    def multiply(self, a, b):
        return a * b

    def divide(self, a, b):
        if b != 0:
            return a / b
        else:
            return "Ошибка: деление на ноль"

calc = Calculator()
a = float(input())
op = input()
b = float(input())

if op == '+':
    result = calc.add(a, b)
elif op == '-':
    result = calc.subtract(a, b)
elif op == '*':
    result = calc.multiply(a, b)
elif op == '/':
    result = calc.divide(a, b)
else:
    result = "Неизвестная операция"

print(result)
