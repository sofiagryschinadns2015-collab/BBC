class Calculator:
    def calculate(self, a, op, b):
        if op == '+':
            return a + b
        elif op == '-':
            return a - b
        elif op == '*':
            return a * b
        elif op == '/':
            if b != 0:
                return a / b
            else:
                return "Ошибка: деление на ноль"
        else:
            return "Неизвестная операция"

calc = Calculator()
a = float(input())
op = input()
b = float(input())

result = calc.calculate(a, op, b)
print(result)
